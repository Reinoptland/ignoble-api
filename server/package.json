{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for ignoble api",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "reset": "npm run migrate:undo:all && npm run migrate && npm run seed:all",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "reseed": "npm run seed:undo:all && npm run seed:all",
    "start": "node index.js",
    "dev": "npx nodemon index.js",
    "heroku-postbuild": "npm run migrate && npm run seed:all"
  },
  "keywords": [
    "ignoble",
    "prize"
  ],
  "author": "Rein Op 't Land",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
